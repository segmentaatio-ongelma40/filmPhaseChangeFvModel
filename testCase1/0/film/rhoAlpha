FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/film";
    object      rhoAlpha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            calculated;
        value           uniform 0;
    }
    walls
    {
        type            calculated;
        value           uniform 0;
    }
    bottom
    {
        type            calculated;
        value           uniform 0;
    }
    wall
    {
        type            calculated;
        value           uniform 688.88685;
    }
    surface
    {
        type            calculated;
        value           uniform 688.88685;
    }
}


